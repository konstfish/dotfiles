cmd + shift - e : open /Applications/Coral.app

cmd - 1 : yabai -m window --toggle zoom-fullscreen

cmd - 2 : yabai -m query --windows --window | grep -q '"has-parent-zoom":true' && \
         yabai -m window --toggle zoom-fullscreen; \
         yabai -m window --warp west

cmd - 3 : yabai -m query --windows --window | grep -q '"has-parent-zoom":true' && \
         yabai -m window --toggle zoom-fullscreen; \
         yabai -m window --warp east

cmd + shift - 1 : yabai -m window --space prev --focus

cmd + shift - 2 : yabai -m window --space next --focus